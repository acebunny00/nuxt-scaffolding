{
	"name": "nuxt-app",
	"private": true,
	"type": "commonjs",
	"scripts": {
		"dev": "npx nuxi dev -o --inspect --host",
		"build": "npx nuxi build",
		"preview": "npx nuxi preview",
		"pm2:start": "npx pm2 start --time",
		"pm2:stop:delete": "npx pm2 stop all && npx pm2 delete all",
		"pm2:reset": "npx pm2 stop all && npx nuxi build && npx pm2 start --time",
		"pm2:monit": "npx pm2 monit",
		"prisma": "npx prisma generate",
		"prisma:pull": "prisma db pull",
		"prisma:sqlite:dev": "npx prisma migrate dev --name init",
		"prisma:sqlite:deploy": "npx prisma migrate deploy",
		"prisma:sqlite:delete": "rd /s /q prisma\\migrations && del /f /q /a prisma\\dev*",
		"prisma:studio": "npx prisma studio",
		"cleanup": "nuxi cleanup",
		"upgrade": "nuxi upgrade",
		"generate": "nuxi generate",
		"postinstall": "nuxi prepare"
	},
	"dependencies": {
		"@editorjs/editorjs": "^2",
		"vue": "^3",
		"vue-router": "^4"
	},
	"devDependencies": {
		"nuxt": "^3"
	},
